@model Models.Entity.Contact

<div class="col-xl-6 col-lg-8 d-flex flex-wrap justify-content-lg-start justify-content-center align-items-center">
    <!--Links start-->
    <div class="header-top-links">
        <ul>
            <li><a href="tel:@Model.MobilePhone"><i class="fa fa-phone"></i>@Model.MobilePhone</a></li>
            <li><a href="mailto:@Model.Email"><i class="fa fa-envelope-open-o"></i>@Model.Email</a></li>
        </ul>
    </div>
    <!--Links end-->
    <!--Socail start-->
    <div class="header-top-social">
        <a href="@Model.LinkFaceBook"><i class="fa fa-facebook"></i></a>
        <a href="@Model.LinkInstagram"><i class="fa fa-instagram"></i></a>
    </div>
    <!--Socail end-->
</div>